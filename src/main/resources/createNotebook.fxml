<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tim.ui.controller.MainController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
   <children>
      <TextField fx:id="notebookNameText" prefWidth="200.0">
        <GridPane.margin><Insets left="10.0" top="10.0" /></GridPane.margin>
      </TextField>
      <Button fx:id="saveNotebookButton" mnemonicParsing="false" text="Save" GridPane.rowIndex="1" />
      <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" GridPane.rowIndex="1" />
   </children>
</GridPane>
